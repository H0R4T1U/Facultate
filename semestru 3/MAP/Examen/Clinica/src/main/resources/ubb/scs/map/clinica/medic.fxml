      <Label fx:id="Header" text="Consultations" />
      <TableView fx:id="tableViewConsultatii" prefHeight="200.0" prefWidth="600.0">
         <columns>
            <TableColumn fx:id="columnId" prefWidth="50.0" text="Id" />
            <TableColumn fx:id="columnCnp" prefWidth="150.0" text="CNP" />
            <TableColumn fx:id="columnNume" prefWidth="125.0" text="Name" />
            <TableColumn fx:id="columnData" prefWidth="155.0" text="Date" />
            <TableColumn fx:id="columnOra" prefWidth="120.0" text="Time" />
         </columns>
      </TableView>
   </VBox>
</AnchorPane>


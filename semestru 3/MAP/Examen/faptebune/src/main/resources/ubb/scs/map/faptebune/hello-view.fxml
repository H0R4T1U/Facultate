<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" spacing="20.0">
          <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </padding>
         <children>
            <Label text="nume" />
            <TextField fx:id="nume" />
            <Label layoutX="95.0" layoutY="30.0" text="prenume" />
            <TextField fx:id="prenume" layoutX="30.0" layoutY="67.0" />
            <Label layoutX="95.0" layoutY="30.0" text="username" />
            <TextField fx:id="username" layoutX="30.0" layoutY="67.0" />
            <Label layoutX="95.0" layoutY="30.0" text="parola" />
            <TextField fx:id="parola" layoutX="30.0" layoutY="67.0" />
            <Label layoutX="95.0" layoutY="279.0" text="oras" />
            <ChoiceBox fx:id="oras" prefWidth="150.0" />
            <Label layoutX="95.0" layoutY="362.0" text="strada" />
            <TextField fx:id="strada" layoutX="30.0" layoutY="399.0" />
            <Label layoutX="95.0" layoutY="445.0" text="numar strada" />
            <TextField fx:id="numarStrada" layoutX="30.0" layoutY="482.0" />
            <Label layoutX="74.0" layoutY="528.0" text="telefon" />
            <TextField fx:id="telefon" layoutX="30.0" layoutY="565.0" />
            <Button mnemonicParsing="false" onAction="#register" text="Submit" />
         </children>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="User" />
            <ChoiceBox fx:id="users" onAction="#login" prefWidth="150.0" />
         </children></VBox>
   </children>
</HBox>
